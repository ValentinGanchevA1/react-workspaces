    "storybook:build": "build-storybook -c workspace-demos/.storybook -o .out",
    "storybook:github": "storybook-to-ghpages",
    "format": "prettier --use-tabs --write \"**/*.{ts,tsx,scss}\""
  },
  "devDependencies": {
    "typescript": "^4.8.3",
    "webpack-cli": "^4.10.0",
    "webpack-node-externals": "^3.0.0"
  }
}
