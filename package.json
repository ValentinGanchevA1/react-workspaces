{
  "name": "@projectstorm/react-workspaces",
  "version": "2.6.1",
  "main": "./dist/main.js",
  "typings": "./dist/@types/src/main",
  "author": "dylanvorster",
  "repository": {
    "type": "git",
    "url": "https://github.com/projectstorm/react-workspaces.git"
  },
  "workspaces": [
    "workspace-*"
  ],
  "keywords": [
    "web",
    "workspace",
    "windows",
    "panels",
    "drag",
    "layout",
    "split",
    "react",
    "typescript",
    "simple"
  ],
  "scripts": {
